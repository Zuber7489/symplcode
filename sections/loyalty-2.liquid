 
<section>
                <div class="weekender_plan_bg">
                    <div class="container">
                        <div class="weekender_plan_f">
                            <div class="main_heading_wrap weekender_heading">
                                <h1>{{ section.settings.heading}}</h1>
                                <p>{{ section.settings.subheading}}</p>
                            </div>

                            <div class="weekender_plan_w loyalty-unpack">
                                 {% for block in section.blocks %}
                  {% if block.settings.heading != blank %}
                                <div class="weekeder_w">
                                    <div class="weekender_text">
                                        <span> <img src="{{ block.settings.image}}"></span>
                                        <div class="weekender_d">
                                            <h3>{{ block.settings.heading}}</h3>
                                           
                                             {{ block.settings.subheading}}
                                          
                                        </div>
                                    </div>
                                </div>

                                 {% endif %}
                  {% endfor %}
                            </div>
                        </div>
                    </div>
                </div>
            </section>

 
